<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pedro.latihanapi">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LatihanApi">
        <activity android:name=".Activity.InfoActivity"></activity>
        <activity android:name=".Activity.MainActivity" />
        <activity android:name=".Activity.MainHome" />
        <activity android:name=".Activity.MainGlobal" />
        <activity android:name=".Activity.MainProvinsi" />
        <activity android:name=".splashScreen.SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>